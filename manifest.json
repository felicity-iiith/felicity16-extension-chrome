{
  "manifest_version": 2,

  "name": "Felicity16",
  "description": "This Chrome Extension notifies you about Felicity16 Events.",
  "version": "1.0",
  "icons": { "16" : "images/felicity16_16.png",
 			"48" : "images/felicity16_48.png",
			"128" : "images/felicity16_128.png"},
  "browser_action": {
    "default_icon": "images/felicity16_128.png",
    "default_popup": "popup.html",
    "default_title": "Felicity16"
  },
  "background": {
    "page" : "background.html"
   },
  "permissions": [
    "notifications",
    "https://felicity.iiit.ac.in/",
    "alarms"
  ]
}
